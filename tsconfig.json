{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es5",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "outDir": "dist"
  }
}
